@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetContacts</title>
</head>
<body>
    <div>
        <table>
        <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Phone No.</th>
            <th>Email Address</th>
        </tr>
        </thead>
        <tbody>
        @foreach (Contact contact in Model)
        {
            <tr>
                <td>@contact.FirstName</td>
                <td>@contact.LastName</td>
                <td>@contact.PhoneNo</td>
                <td>@contact.EmailAddress</td>
            </tr>
        }
        </tbody>
        </table>
</div>
</body>
</html>
