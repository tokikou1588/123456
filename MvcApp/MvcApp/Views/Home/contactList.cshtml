@model IEnumerable<Contact>

<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Phone No.</th>
            <th>Email Address</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Contact contact in Model)
        { 
            <tr>
                <td>@contact.FirstName</td>
                 <td>@contact.LastName</td>
                 <td>@contact.PhoneNo</td>
                 <td>@contact.EmailAddress</td>
            </tr>
        }
    </tbody>
</table>